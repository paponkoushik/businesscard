					</svg>
				</div>
				<div class="mid-bottom">
					<h4>{{ module.mid_number.translatable|striptags('<span>')|raw }}</h4>
					<p>{{ module.mid_text.translatable|striptags('<span>')|raw }}</p>
				</div>
			</div>

			<div class="contents right-content">
				<h4>{{ module.right_title.translatable|striptags('<span>')|raw }}</h4>
				<p>{{ module.right_discription.translatable|striptags('<span>')|raw }}</span></p>

