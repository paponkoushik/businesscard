{% extends 'partials/variables.html.twig' %}

{% set module = page.header.module %}

{% block render_block %}

<div class="about_features">
	<div class="container">
		<div class="about_features-header-section">
			{% if module.subtitle %}
				<h4>
					<span>{{ module.subtitle.translatable }}</span>
				</h4>
			{% endif %}

			{% if module.title %}
				<h3 class="block-title">{{ module.title.translatable|striptags('<span>')|raw }}</h3>
			{% endif %}

			<div class="description">
				{% if module.description %}
					<p>{{ module.description.translatable }}</p>
				{% endif %}
			</div>
		</div>

		<div class="about_features-container">

			<div class="contents left-content">
				<h4>{{ module.left_title.translatable|striptags('<span>')|raw }}</h4>
				<img src="{{ url('theme://images/about-features/about-features-1.svg') }}" />
				<h4>{{ module.left_discription.translatable|striptags('<span>')|raw }}</h4>
			</div>

			<div class="contents mid-content">
				<h4>{{ module.mid_title.translatable|striptags('<span>')|raw }}</h4>
				<p>{{ module.mid_discription.translatable|striptags('<span>')|raw }}</p>

				<div class="mid-rate">
					<h3>{{ module.mid_rate.translatable|striptags('<span>')|raw }}</h3>
					<svg xmlns="http://www.w3.org/2000/svg" width="37" height="37" viewBox="0 0 37 37" fill="none">
						<circle cx="18.285" cy="18.4767" r="14.5312" transform="rotate(-16.9756 18.285 18.4767)" fill="#FAFAFA"/>
